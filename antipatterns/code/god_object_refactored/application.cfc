<cfcomponent>	<cfset this.name = "god_object_refactored" />	<cffunction name="onApplicationStart" access="remote">			<cfscript>		application.URLRoot = "http://antipatterns.local/god_object_refactored/";				application.ComponentRoot = "http://antipatterns.local/god_object_refactored/cfc/";				application.datasource="inventory";		application.componentPrefix = "god_object_refactored.cfc.";				application.imagePrefix = "http://antipatterns.local/god_object_refactored/images/";		application.apartment = createObject("component", "#application.componentPrefix#Apartment").init();			 </cfscript></cffunction>	<cffunction name="onRequestStart" access="remote">			<cfif StructKeyExists(url, 'init') >					<cfscript>    		onApplicationStart();    	</cfscript>	   		</cfif>	   	</cffunction>	</cfcomponent>